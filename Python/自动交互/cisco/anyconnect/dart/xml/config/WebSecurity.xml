            <plugin></plugin>
            <args>wineventlog.exe -l "Cisco AnyConnect Web Security Module" -f %TEMP%\WebSecurity.txt</args>
            <clear_log apply="false"></clear_log>
            <temp_out>%TEMP%\WebSecurity.txt</temp_out>
          </action>
          <action>
            <path>%WINDIR%\system32</path>
            <args>wevtutil epl "Cisco AnyConnect Web Security Module" %TEMP%\WebSecurity.evtx</args>
            <clear_log apply="true">wevtutil cl "Cisco AnyConnect Web Security Module"</clear_log>
            <temp_out>%TEMP%\WebSecurity.evtx</temp_out>
          </action>
        </use_extern_action>
      </os>
      <os opsys="winxp">
        <use_extern_action>
          <action>
            <plugin></plugin>
            <args>wineventlog.exe -l "Cisco AnyConnect Web Security Module" -f %TEMP%\WebSecurity.txt</args>
            <clear_log apply="false"></clear_log>
            <temp_out>%TEMP%\WebSecurity.txt</temp_out>
          </action>
          <action>
            <path>%WINDIR%\system32\wbem</path>
            <args>wmic.exe nteventlog where " logfilename='Cisco AnyConnect Web Security Module' " call backupeventlog "%TEMP%\WebSecurity.evt"</args>
            <clear_log apply="true">wmic.exe nteventlog where " logfilename='Cisco AnyConnect Web Security Module' " call ClearEventLog</clear_log>
            <temp_out>%TEMP%\WebSecurity.evt</temp_out>
          </action>
        </use_extern_action>
      </os>
      <os opsys="win2k3">
